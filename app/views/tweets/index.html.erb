<h1>Tabla Tweets</h1>

<table>
  <thead>
    <tr>
      <th>Id tweet</th>
      <th>Id texto</th>
      <th>Id usuario</th>
      <th>Region</th>
      <th>Fecha</th>
      <th>Clasificacion</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @tweets.each do |tweet| %>
      <tr>
        <td><%= tweet.id_tweet %></td>
        <td><%= tweet.id_texto %></td>
        <td><%= tweet.id_usuario %></td>
        <td><%= tweet.region %></td>
        <td><%= tweet.fecha %></td>
        <td><%= tweet.clasificacion %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

